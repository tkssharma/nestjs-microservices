{
  "name": "@tkssharma/database",
  "version": "1.0.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "check-engines": "node ../../node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "clean": "rimraf build && rimraf coverage",
    "prebuild": "npm run clean && npm run check-engines",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test:ci",
    "test:ci": "jest --ci --collectCoverage --maxWorkers 2 --passWithNoTests"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "dependencies": {
    "@nestjs/typeorm": "^9.0.1",
    "typeorm": "^0.3.12",
    "uuid": "^8.3.2",
    "@tkssharma/config": "*"
  },
  "devDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "tslib": "^2.5.0",
    "typescript": "^4.7.4"
  }
}
